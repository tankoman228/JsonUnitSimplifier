{
  "id": "No id, I am lazy",
  "mode": "fields",
  "combination_mode": "simple",
  "classes": [ "Namespace.NoNeedInThisSituation"],
  "rules": [
    {
      "field": "FieldInt",
	  "field_type": "System.Int32",
      "values": [ 1, 2, 3, 4, 5 ]
    },
    {
      "field": "FieldFloat",
	  "field_type": "System.Single",
      "value": -23.2
    },
    {
      "field": "FieldDouble",
      "range": [ -1, 1 ],
      "step": 0.5
    },
    {
      "field": "FieldString",
      "function": "created_function"
    }
  ],
  "assert_before_lambda": [
  {
	"function": "ExceptNotImplementedIfZeroOrReturnZero",
	"type_assert": "unequals",
	"args": [0],
	"result": 1,
	"exception": null
  },
  {
	"function": "ExceptNotImplementedIfZeroOrReturnZero",
	"type_assert": "equals",
	"args": [1],
	"type_assert": "equals",
	"result": null,
	"exception": "NotImplementedException"
  }
  ]
}