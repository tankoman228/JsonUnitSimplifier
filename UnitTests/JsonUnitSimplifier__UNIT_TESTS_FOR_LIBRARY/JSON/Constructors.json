{
  "id": "No id, I am lazy",
  "mode": "constructor",
  "combination_mode": "simple",
  "classes": [ "Namespace.NoNeedInThisSituation" ],
  "rules": [
    {
      "field_type": "System.Int32",
      "range": [ -2, 2 ],
      "step": 1
    },
    {
      "field_type": "System.Single",
      "value": -23.2
    },
    {
      "range": [ -1, 1 ],
      "step": 0.5
    },
    {
      "random": "[a3][d3][l3][a3]"
    }
  ],
  "assert_after_lambda": [
    {
      "field": "FieldString",
      "type_assert": "regex",
      "value": "\\d\\d\\d\\w\\w\\w"
    }
  ]
}